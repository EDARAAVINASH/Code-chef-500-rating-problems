import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner Scanner=new Scanner(System.in);
        int n=Scanner.nextInt();
        for(int i=0;i<n;i++)
        {
            int a=Scanner.nextInt();
            int sum=0;
            while(a>0)
            {
                int b=a%10;
                sum=sum+b;
                a=a/10;
            }
            System.out.println(sum);
        }
	}
}
