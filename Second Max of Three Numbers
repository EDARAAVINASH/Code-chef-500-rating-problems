import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        for(int j=0;j<n;j++)
        {
            int a[]=new int[3];
            for(int i=0;i<3;i++)
            {
                a[i]=scan.nextInt();
            }
            Arrays.sort(a);
            System.out.println(a[1]);
        }
	}
}
