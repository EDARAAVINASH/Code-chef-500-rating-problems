import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        for(int i=0;i<n;i++)
        {
            int a=scan.nextInt();
            if(a>250)
            {
                System.out.println("NO");
            }
            else
            {
                System.out.println("YES");
            }
        }
	}
}
