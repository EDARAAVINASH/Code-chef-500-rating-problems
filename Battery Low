import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner Scanner=new Scanner(System.in);
        int n=Scanner.nextInt();
        for(int i=0;i<n;i++)
        {
            int n1=Scanner.nextInt();
            if(n1<=15)
            {
                System.out.println("Yes");
            }
            else
            {
                System.out.println("No");
            }
        }
	}
}
