import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        if(n>=6 & n<=8)
        {
            System.out.print("Yes");
        }
        else
        {
            System.out.print("No");
        }
	}
}
