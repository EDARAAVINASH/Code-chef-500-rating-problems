import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner Scanner=new Scanner(System.in);
        int n=Scanner.nextInt();
        int m=Scanner.nextInt();
        int height=m>n?m:n;
        System.out.println(height);
	}
}
